{
  "expo": {
    "name": "clean-shop",
    "slug": "clean-shop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "cleanshop",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.killerchip.cleanshop"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.killerchip.cleanshop"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "@config-plugins/detox"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f7e6f665-5d17-4f3d-bb86-5e496ac11b60"
      }
    }
  }
}
